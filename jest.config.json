{
  "setupFiles": ["<rootDir>/setupTests.js"],
  "testRegex": "/*.test.tsx$",
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "coverage",
  "coverageThreshold": {
    "global": {
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "statements": 0
    }
  },
  "moduleDirectories": ["node_modules", "src"],
  "transformIgnorePatterns": [
    "<rootDir>/server/node_modules/"
  ],
  "preset": "ts-jest",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}