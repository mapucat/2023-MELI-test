{
  "name": "2023-meli-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.3.3",
    "concurrently": "^7.6.0",
    "http-proxy-middleware": "^2.0.6",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-currency-format": "^1.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "sort-by": "^1.2.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "install:all": "concurrently \"npm run install:server\" \"npm install\"",
    "install:server": "npm --prefix server install",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "react-scripts start",
    "start:server": "npm --prefix server start",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:client": "react-scripts build",
    "build:server": "npm --prefix server build",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:client": "react-scripts test",
    "test:server": "npm --prefix server test",
    "coverage": "concurrently \"npm run coverage:server\" \"npm run coverage:client\"",
    "coverage:client": "react-scripts test --coverage",
    "coverage:server": "npm --prefix server test --coverage",
    "cypress:open": "concurrently \"npm run start\" \"cypress open\"",
    "cypress:headless": "concurrently \"npm run start\" \"cypress run\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/chai": "^4.3.4",
    "@types/jest": "^27.5.2",
    "@types/mocha": "^10.0.1",
    "@types/react-currency-format": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "chai": "^4.3.7",
    "cypress": "^12.5.1",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^27.5.1",
    "mocha": "^10.2.0",
    "sass": "^1.58.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "webpack": "^5.75.0"
  }
}
